{
  "Student": {
    "special_char":"s",
    "initial": "Student_intelligence",
    "interactions": {
      "Student_intelligence": {
        "prompt": "你遇到了一位同学。",
        "options": [
          {
            "id": "Student_teach",
            "text": "请教问题",
            "next": "Student_teach_result"
          },
          {
            "id": "Student_leave",
            "text": "离开",
            "next": "END"
          }
        ]
      },
      "Student_teach_result": {
        "prompt": "通过请教，你的文科智力提升了5点。",
        "options": [],
        "next": "END",
        "effect": {
          "TIME": "-2",
          "INTEL_ARTS": "5"
        }
      },
      "END": {
        "prompt": "对话结束",
        "options": []
      }
    }
  },
  
