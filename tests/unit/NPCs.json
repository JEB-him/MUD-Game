      },
      "Student_teach_result": {
        "prompt": "通过请教，你的文科智力提升了5点。",
        "options": [ ],
        "next": "END",
        "effect": {
          "INTEL_ARTS": "5"
        }
      },
      "END": {
        "prompt": "对话结束",
        "options": []
      }
    }
